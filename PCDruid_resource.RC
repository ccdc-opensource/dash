    POPUP "&View"
    BEGIN
        MENUITEM "&Diffraction Setup",                       ID_get_data_properties
        MENUITEM "&Peak Positions",                          ID_get_peak_positions
        MENUITEM "Cell Parameters",                          ID_get_crystal_symmetry
        MENUITEM "Peak &Widths",                             ID_get_peak_widths
        MENUITEM "Pawley / SA",                              IDM_ViewPawley
    END
    POPUP "&Options"
    BEGIN
        MENUITEM "Plot Options...",                          ID_plot_options
        MENUITEM "Configuration...",                         ID_Configuration
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "DASH &Help\aCtrl-H",                       ID_PolyFitter_Help
        MENUITEM SEPARATOR
        POPUP "DASH &Tutorials"
        BEGIN
            MENUITEM "Tutorial 1: Hydrochlorothiazide",          ID_Tutorial_1
            MENUITEM "Tutorial 2: Chloropropamide",              ID_Tutorial_2
            MENUITEM "Tutorial 3: Cimetidine",                   ID_Tutorial_3
            MENUITEM "Tutorial 4: Decafluoroquaterphenyl",       ID_Tutorial_4
            MENUITEM "Tutorial 5: Uridine",                      ID_Tutorial_5
        END
        MENUITEM SEPARATOR
        MENUITEM "About &DASH...",                           ID_help_about_Polyfitter
    END
END

